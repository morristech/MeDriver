<layout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	>
	<data>
		<variable
			name="viewModel"
			type="com.mmdev.me.driver.presentation.ui.fuel.history.FuelHistoryViewModel"
			/>
	</data>
	
	<androidx.constraintlayout.widget.ConstraintLayout
		
		android:layout_height="match_parent"
		android:layout_width="match_parent"
		android:focusableInTouchMode="true"
		
		>
		<!-- card 1 distance passed -->
		<com.google.android.material.card.MaterialCardView
			android:id="@+id/cvDistancePassed"
			android:layout_height="wrap_content"
			android:layout_width="0dp"
			android:layout_marginTop="8dp"
			android:layout_marginEnd="4dp"
			
			app:cardCornerRadius="6dp"
			app:cardElevation="2dp"
			app:contentPadding="8dp"
			
			app:layout_constraintStart_toStartOf="@id/guidelineStart"
			app:layout_constraintEnd_toStartOf="@id/guidelineCenter"
			app:layout_constraintTop_toTopOf="parent"
			
			>
			
			<androidx.constraintlayout.widget.ConstraintLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				>
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvDistancePassedTitle"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					
					android:text="Distance passed"
					android:textAppearance="?textAppearanceCaption"
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					app:layout_constraintBottom_toTopOf="@id/tvDistancePassedValue"
					/>
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvDistancePassedValue"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					
					android:maxLines="1"
					
					android:text="@{`+` + viewModel.distancePassed + ` km`, default = `0 km`}"
					android:textAppearance="?textAppearanceHeadline5"
					android:textColor="?colorOnSurface"
					
					app:autoSizeTextType="uniform"
					app:autoSizeMinTextSize="14sp"
					app:autoSizeMaxTextSize="28sp"
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tvDistancePassedTitle"
					
					/>
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvDistancePassedSubtitle"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					
					android:text="@{viewModel.historyRecord.odometerValue + ` km`, default = `0 km`}"
					android:textAppearance="?textAppearanceCaption"
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tvDistancePassedValue"
					app:layout_constraintBottom_toBottomOf="parent"
					/>
			
			</androidx.constraintlayout.widget.ConstraintLayout>
		
		</com.google.android.material.card.MaterialCardView>
		
		<!-- card 2 consumption -->
		<com.google.android.material.card.MaterialCardView
			android:id="@+id/cvConsumption"
			android:layout_height="wrap_content"
			android:layout_width="0dp"
			android:layout_marginTop="8dp"
			android:layout_marginStart="4dp"
			
			app:cardCornerRadius="6dp"
			app:contentPadding="8dp"
			app:cardElevation="2dp"
			
			app:layout_constraintStart_toEndOf="@id/guidelineCenter"
			app:layout_constraintEnd_toEndOf="@id/guidelineEnd"
			app:layout_constraintTop_toTopOf="parent"
			
			>
			
			<androidx.constraintlayout.widget.ConstraintLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent">
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvConsumptionTitle"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					
					
					android:text="Consumption"
					android:textAppearance="?textAppearanceCaption"
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					app:layout_constraintBottom_toTopOf="@id/tvConsumptionValue"
					/>
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvConsumptionValue"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					
					android:maxLines="1"
					
					android:text="@{viewModel.fuelConsumptionValue + ` L`, default = `0.0 L`}"
					android:textAppearance="?textAppearanceHeadline5"
					
					app:autoSizeTextType="uniform"
					app:autoSizeMinTextSize="14sp"
					app:autoSizeMaxTextSize="28sp"
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toStartOf="@id/ivFuelConsumptionChangesIcon"
					app:layout_constraintTop_toBottomOf="@id/tvConsumptionTitle"
					/>
				
				<ImageView
					android:id="@+id/ivFuelConsumptionChangesIcon"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					
					android:src="@{viewModel.fuelConsumptionDrawable}"
					
					app:layout_constraintTop_toTopOf="@id/tvConsumptionValue"
					app:layout_constraintBottom_toBottomOf="@id/tvConsumptionValue"
					app:layout_constraintStart_toEndOf="@id/tvConsumptionValue"
					app:layout_constraintEnd_toEndOf="parent"
					android:contentDescription="@null"
					/>
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvConsumptionPer100kmTitle"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
				
					
					android:text="/100 km"
					android:textAppearance="?textAppearanceCaption"
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tvConsumptionValue"
					app:layout_constraintBottom_toBottomOf="parent"
					/>
			
			
			</androidx.constraintlayout.widget.ConstraintLayout>
		
		
		</com.google.android.material.card.MaterialCardView>
		
		<!-- card 3 total cost -->
		<com.google.android.material.card.MaterialCardView
			android:id="@+id/cvTotalCost"
			android:layout_height="wrap_content"
			android:layout_width="0dp"
			android:layout_marginEnd="4dp"
			
			app:contentPadding="8dp"
			app:cardCornerRadius="6dp"
			app:cardElevation="2dp"
			
			app:layout_constraintStart_toStartOf="@id/guidelineStart"
			app:layout_constraintEnd_toStartOf="@id/guidelineCenter"
			app:layout_constraintTop_toBottomOf="@id/barrier1CardsHorizontal"
			
			>
			
			<androidx.constraintlayout.widget.ConstraintLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent">
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvTotalCostTitle"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					
					android:text="Estimated cost"
					android:textAppearance="?textAppearanceCaption"
					
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					app:layout_constraintBottom_toTopOf="@id/tvTotalCostValue"
					/>
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvTotalCostValue"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					
					android:maxLines="1"
					android:maxLength="10"
					
					android:text="@{viewModel.totalCostValue + ` ₴`, default = `0.0 ₴`}"
					android:textAppearance="?textAppearanceHeadline5"
					
					app:autoSizeTextType="uniform"
					app:autoSizeMinTextSize="14sp"
					app:autoSizeMaxTextSize="28sp"
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tvTotalCostTitle"
					/>
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvTotalCostLiters"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					
					android:maxLines="1"
					android:maxLength="15"
					
					android:text="@{viewModel.sliderLitersValue + ` liters` , default = `1.0 liters`}"
					android:textAppearance="?textAppearanceCaption"
					
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tvTotalCostValue"
					app:layout_constraintBottom_toBottomOf="parent"
					/>
			
			
			</androidx.constraintlayout.widget.ConstraintLayout>
		
		
		</com.google.android.material.card.MaterialCardView>
		
		<!-- card 4 estimate distance -->
		<com.google.android.material.card.MaterialCardView
			android:id="@+id/cvCard4"
			android:layout_height="wrap_content"
			android:layout_width="0dp"
			android:layout_marginStart="4dp"
			
			app:contentPadding="8dp"
			app:cardCornerRadius="6dp"
			app:cardElevation="2dp"
			
			app:layout_constraintStart_toEndOf="@id/guidelineCenter"
			app:layout_constraintEnd_toEndOf="@id/guidelineEnd"
			app:layout_constraintTop_toBottomOf="@id/barrier1CardsHorizontal"
			>
			
			<androidx.constraintlayout.widget.ConstraintLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				>
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvCard3Title"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					
					android:text="Estimate distance"
					android:textAppearance="?textAppearanceCaption"
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					app:layout_constraintBottom_toTopOf="@id/tvCard3Value"
					/>
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvCard3Value"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					
					android:text="~0 km"
					android:textAppearance="?textAppearanceHeadline5"
					android:textColor="?colorOnSurface"
					
					app:autoSizeTextType="uniform"
					app:autoSizeMinTextSize="14sp"
					app:autoSizeMaxTextSize="28sp"
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tvCard3Title"
					/>
				
				<com.google.android.material.textview.MaterialTextView
					android:id="@+id/tvCard3Subtitle"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					
					android:text="/100 km"
					android:textAppearance="?textAppearanceCaption"
					
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tvCard3Value"
					app:layout_constraintBottom_toBottomOf="parent"
					/>
			
			</androidx.constraintlayout.widget.ConstraintLayout>
		
		</com.google.android.material.card.MaterialCardView>
		
		
		
		
		<!-- drop car chooser-->
		<com.google.android.material.textfield.TextInputLayout
			android:id="@+id/layoutInputFuelStation"
			android:layout_height="wrap_content"
			android:layout_width="0dp"
			android:layout_marginEnd="4dp"
			android:focusable="false"
			
			android:hint="Fuel Station"
			
			app:errorEnabled="@{viewModel.fuelStationRequires}"
			
			style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense.ExposedDropdownMenu"
			
			app:layout_constraintTop_toBottomOf="@id/barrier2CardsHorizontal"
			app:layout_constraintStart_toStartOf="@id/guidelineStart"
			app:layout_constraintEnd_toStartOf="@id/guidelineCenter"
			>
			
			<com.google.android.material.textfield.MaterialAutoCompleteTextView
				android:id="@+id/etFuelStationDrop"
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				
				android:imeOptions="actionDone"
				android:inputType="textNoSuggestions"
				
				android:maxLength="15"
				android:maxLines="1"
				
				android:text="@={viewModel.fuelStationInputValue}"
				app:hideKeyboardOnInputDone="@{true}"
				/>
		
		</com.google.android.material.textfield.TextInputLayout>
		
		<!-- price input -->
		<com.google.android.material.textfield.TextInputLayout
			android:id="@+id/layoutInputPrice"
			android:layout_height="wrap_content"
			android:layout_width="0dp"
			android:layout_marginStart="4dp"
			android:focusable="false"
			
			android:hint="Price"
			app:errorEnabled="@{viewModel.fuelPriceRequires}"
			app:suffixText="₴"
			
			style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
			
			app:layout_constraintStart_toEndOf="@id/guidelineCenter"
			app:layout_constraintEnd_toEndOf="@id/guidelineEnd"
			app:layout_constraintTop_toBottomOf="@id/barrier2CardsHorizontal"
			
			>
			
			<com.google.android.material.textfield.TextInputEditText
				android:id="@+id/etInputPrice"
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				
				android:imeOptions="actionDone"
				android:inputType="numberDecimal"
				android:maxLength="5"
				android:maxLines="1"
				
				android:text="@={viewModel.priceInputValue}"
				
				app:hideKeyboardOnInputDone="@{true}"
				/>
		
		
		</com.google.android.material.textfield.TextInputLayout>
		
		
		<com.google.android.material.textview.MaterialTextView
			android:id="@+id/tvFuelTypeHint"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:paddingBottom="8dp"
			
			android:text="Select fuel type"
			android:textAlignment="center"
			android:textAppearance="?textAppearanceBody2"
			
			app:layout_constraintStart_toStartOf="@id/guidelineStart"
			app:layout_constraintEnd_toEndOf="@id/guidelineEnd"
			app:layout_constraintTop_toBottomOf="@id/barrierFuelStationHorizontal"
			
			/>
		
		<!-- gas -->
		<com.google.android.material.button.MaterialButton
			android:id="@+id/btnFuelTypeGas"
			android:layout_width="@dimen/btn_fuel_type_size"
			android:layout_height="@dimen/btn_fuel_type_size"
			
			android:text="GAS"
			android:textSize="12sp"
			
			app:backgroundTint="@color/btn_fuel_tint_bg"
			
			style="@style/My.Widget.Button.LetterOnly.Rounded"
			
			app:layout_constraintStart_toStartOf="@id/guidelineStart"
			app:layout_constraintEnd_toStartOf="@id/btnFuelTypeDT"
			app:layout_constraintTop_toBottomOf="@id/tvFuelTypeHint"
			
			/>
		
		<!-- dt -->
		<com.google.android.material.button.MaterialButton
			android:id="@+id/btnFuelTypeDT"
			android:layout_width="@dimen/btn_fuel_type_size"
			android:layout_height="@dimen/btn_fuel_type_size"
			
			android:text="DT"
			android:textSize="12sp"
			
			app:backgroundTint="@color/btn_fuel_tint_bg"
			
			style="@style/My.Widget.Button.LetterOnly.Rounded"
			
			app:layout_constraintStart_toEndOf="@id/btnFuelTypeGas"
			app:layout_constraintEnd_toStartOf="@id/btnFuelType92"
			app:layout_constraintTop_toBottomOf="@id/tvFuelTypeHint"
			
			/>
		
		<!-- 92 -->
		<com.google.android.material.button.MaterialButton
			android:id="@+id/btnFuelType92"
			android:layout_width="@dimen/btn_fuel_type_size"
			android:layout_height="@dimen/btn_fuel_type_size"
			
			android:text="92"
			android:textSize="12sp"
			
			app:backgroundTint="@color/btn_fuel_tint_bg"
			
			style="@style/My.Widget.Button.LetterOnly.Rounded"
			
			app:layout_constraintStart_toEndOf="@id/btnFuelTypeDT"
			app:layout_constraintEnd_toStartOf="@id/btnFuelType95"
			app:layout_constraintTop_toBottomOf="@id/tvFuelTypeHint"
			
			/>
		
		<!-- 95 -->
		<com.google.android.material.button.MaterialButton
			android:id="@+id/btnFuelType95"
			android:layout_width="@dimen/btn_fuel_type_size"
			android:layout_height="@dimen/btn_fuel_type_size"
			
			android:text="95"
			android:textSize="12sp"
			
			app:backgroundTint="@color/btn_fuel_tint_bg"
			
			
			style="@style/My.Widget.Button.LetterOnly.Rounded"
			
			app:layout_constraintStart_toEndOf="@id/btnFuelType92"
			app:layout_constraintEnd_toStartOf="@id/btnFuelType95PLUS"
			app:layout_constraintTop_toBottomOf="@id/tvFuelTypeHint"
			
			/>
		
		<!-- 95plus -->
		<com.google.android.material.button.MaterialButton
			android:id="@+id/btnFuelType95PLUS"
			android:layout_width="@dimen/btn_fuel_type_size"
			android:layout_height="@dimen/btn_fuel_type_size"
			
			android:text="95+"
			android:textSize="12sp"
			
			app:backgroundTint="@color/btn_fuel_tint_bg"
			
			style="@style/My.Widget.Button.LetterOnly.Rounded"
			
			app:layout_constraintStart_toEndOf="@id/btnFuelType95"
			app:layout_constraintEnd_toStartOf="@id/btnFuelType100"
			app:layout_constraintTop_toBottomOf="@id/tvFuelTypeHint"
			
			/>
		
		<!-- 100 -->
		<com.google.android.material.button.MaterialButton
			android:id="@+id/btnFuelType100"
			android:layout_width="@dimen/btn_fuel_type_size"
			android:layout_height="@dimen/btn_fuel_type_size"
			
			android:text="100"
			android:textSize="12sp"
			
			app:backgroundTint="@color/btn_fuel_tint_bg"
			
			style="@style/My.Widget.Button.LetterOnly.Rounded"
			
			app:layout_constraintStart_toEndOf="@id/btnFuelType95PLUS"
			app:layout_constraintEnd_toEndOf="@id/guidelineEnd"
			app:layout_constraintTop_toBottomOf="@id/tvFuelTypeHint"
			
			/>
		
		
		<!-- odometer input -->
		<com.google.android.material.textfield.TextInputLayout
			android:id="@+id/layoutInputOdometer"
			android:layout_height="wrap_content"
			android:layout_width="0dp"
			android:focusable="false"
			
			android:hint="Enter odometer value"
			app:errorEnabled="@{viewModel.odometerRequires}"
			app:suffixText="km"
			
			style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
			
			app:layout_constraintStart_toStartOf="@id/guidelineStart"
			app:layout_constraintEnd_toEndOf="@id/guidelineEnd"
			app:layout_constraintTop_toBottomOf="@id/barrierFuelTypesHorizontal"
			app:layout_constraintBottom_toTopOf="@id/sliderLiters"
			
			>
			
			<com.google.android.material.textfield.TextInputEditText
				android:id="@+id/etInputOdometer"
				android:layout_height="match_parent"
				android:layout_width="match_parent"
				
				android:imeOptions="actionDone"
				android:inputType="number"
				android:maxLength="6"
				android:maxLines="1"
				
				android:text="@={viewModel.odometerInputValue}"
				android:textAlignment="center"
				
				app:hideKeyboardOnInputDone="@{true}"
				/>
		
		
		</com.google.android.material.textfield.TextInputLayout>
		
		<!-- liters slider -->
		<com.google.android.material.slider.Slider
			android:id="@+id/sliderLiters"
			android:layout_height="wrap_content"
			android:layout_width="0dp"
			android:layout_marginTop="8dp"
			
			android:value="@={viewModel.sliderLitersValue}"
			android:valueFrom="1"
			android:valueTo="70"
			android:stepSize="0.1"
			
			app:labelBehavior="gone"
			
			app:layout_constraintTop_toBottomOf="@id/layoutInputOdometer"
			app:layout_constraintBottom_toTopOf="@id/tvLitersValue"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			/>
		
		
		<!-- liters value -->
		<com.google.android.material.textview.MaterialTextView
			android:id="@+id/tvLitersValue"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:labelFor="@id/sliderLiters"
			
			android:maxLines="1"
			android:maxLength="6"
			
			android:text="@{viewModel.sliderLitersValue + ` L` , default = `1.0 L`}"
			android:textAlignment="center"
			android:textAppearance="?textAppearanceHeadline4"
			android:textColor="?colorOnBackground"
			
			app:layout_constraintStart_toStartOf="@id/guidelineStart"
			app:layout_constraintEnd_toEndOf="@id/guidelineEnd"
			app:layout_constraintTop_toBottomOf="@id/sliderLiters"
			
			/>
		
		
		
		
		<!-- cancel -->
		<com.google.android.material.button.MaterialButton
			android:id="@+id/btnCancel"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			
			
			android:text="Cancel"
			
			style="@style/Widget.MaterialComponents.Button.OutlinedButton"
			
			app:layout_constraintStart_toStartOf="@id/guidelineStart"
			app:layout_constraintEnd_toStartOf="@id/guidelineCenter"
			app:layout_constraintTop_toBottomOf="@id/barrierFuelPriceHorizontal"
			app:layout_constraintBottom_toBottomOf="parent"
			/>
		
		<!-- done -->
		<com.google.android.material.button.MaterialButton
			android:id="@+id/btnDone"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:enabled="@{viewModel.allFieldsAreCorrect}"
			
			android:text="Done"
			
			app:layout_constraintStart_toEndOf="@id/guidelineCenter"
			app:layout_constraintEnd_toEndOf="@id/guidelineEnd"
			app:layout_constraintTop_toBottomOf="@id/barrierFuelPriceHorizontal"
			app:layout_constraintBottom_toBottomOf="parent"
			/>
		
		<androidx.constraintlayout.widget.Barrier
			android:id="@+id/barrier1CardsHorizontal"
			android:layout_width="0dp"
			android:layout_height="0dp"
			app:barrierMargin="8dp"
			app:barrierDirection="bottom"
			app:constraint_referenced_ids="cvConsumption, cvDistancePassed"
			/>
		
		<androidx.constraintlayout.widget.Barrier
			android:id="@+id/barrier2CardsHorizontal"
			android:layout_width="0dp"
			android:layout_height="0dp"
			app:barrierMargin="8dp"
			app:barrierDirection="bottom"
			app:constraint_referenced_ids="cvCard4, cvTotalCost"
			/>
		
		<androidx.constraintlayout.widget.Barrier
			android:id="@+id/barrierFuelStationHorizontal"
			android:layout_width="0dp"
			android:layout_height="0dp"
			app:barrierMargin="8dp"
			app:barrierDirection="bottom"
			app:constraint_referenced_ids="layoutInputFuelStation, layoutInputPrice"
			/>
		
		<androidx.constraintlayout.widget.Barrier
			android:id="@+id/barrierFuelTypesHorizontal"
			android:layout_width="0dp"
			android:layout_height="0dp"
			app:barrierMargin="8dp"
			app:barrierDirection="bottom"
			app:constraint_referenced_ids="btnFuelTypeGas, btnFuelTypeDT, btnFuelType92, btnFuelType95, btnFuelType95PLUS, btnFuelType100"
			/>
		
		<androidx.constraintlayout.widget.Barrier
			android:id="@+id/barrierFuelPriceHorizontal"
			android:layout_width="0dp"
			android:layout_height="0dp"
			app:barrierMargin="8dp"
			app:barrierDirection="bottom"
			app:constraint_referenced_ids="tvLitersValue"/>
		
		<!-- center -->
		<androidx.constraintlayout.widget.Guideline
			android:id="@+id/guidelineCenter"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:layout_marginHorizontal="4dp"
			android:layout_margin="4dp"
			android:orientation="vertical"
			app:layout_constraintGuide_percent="0.5"
			/>
		
		<!-- start -->
		<androidx.constraintlayout.widget.Guideline
			android:id="@+id/guidelineStart"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:orientation="vertical"
			app:layout_constraintGuide_begin="8dp"
			/>
		
		<!-- end -->
		<androidx.constraintlayout.widget.Guideline
			android:id="@+id/guidelineEnd"
			android:layout_width="0dp"
			android:layout_height="match_parent"
			android:orientation="vertical"
			app:layout_constraintGuide_end="8dp"
			/>
		
	</androidx.constraintlayout.widget.ConstraintLayout>
	
</layout>